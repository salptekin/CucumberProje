package day29_collections;

import java.util.HashMap;
import java.util.Set;
import java.util.Iterator;

public class HashMap01 {

	public static void main(String[] args) {
		
		 HashMap<Integer,String> hashmap = new HashMap<Integer,String>();
	       hashmap.put(100, "Anjali"); //adding key value pairs
	       hashmap.put(101, "Anu");
	       hashmap.put(102, "Banu");
	       System.out.println(hashmap); // Prints the list of key,value pairs
	       
	       Set entrySet = hashmap.entrySet(); 
	       Iterator<Integer> it = entrySet.iterator();      //printing using Iterator
//	 
//	       while(it.hasNext()) 
//	       {   Integer key = it.next();
//	           System.out.println(stud.get(key));
//	       }

	}

}
